<%= "pdf_create.html" %>

<table>
  <tr>
    <th>id</th>
    <th>Particular</th>
    <th>Description</th>
    <th>Amount</th>
    
  </tr>

  <% for income in @incomes %>

  <tr>
    <td><%= income.id %> </td>
    <td><%= income.particular %> </td>
    <td><%= income.description %> </td>
    <td><%= income.amount %> </td>
    
  </tr>

  <% end %>
  
</table>

<p><%= link_to "Create PDF", :action=>"pdf_create" %></p>